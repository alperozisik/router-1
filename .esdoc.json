{
  "source": "./src",
  "destination": "./docs",
  "plugins": [
    {
      "name": "esdoc-ecmascript-proposal-plugin",
      "option": {
        "all": true
      }
    },
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "brand": {
          "title": "Smmartface",
          "description": "Smartface NativeRouter",
          "repository": "https://github.com/smartface/router",
          "site": "http://smartface.io",
          "author": "https://github.com/cenkce"
        },
        "test": {
          "source": "./__test__",
          "interfaces": [
            "expect",
            "test",
            "describe",
            "it"
          ],
          "includes": [
            "(spec|test)\\.js$"
          ]
        }
      }
    }
  ]
}